{
  "mcpServers": {
    "amap": {
      "command": "npx",
      "description": "高德地图 MCP 服务",
      "args": [
        "-y",
        "@amap/amap-maps-mcp-server"
      ],
      "env": {
        "AMAP_MAPS_API_KEY": "your-api-key"
      },
      "tools": {
        "amap-maps_regeocode": {
          "enabled": true
        }
      },
      "enabled": false
    },
    "playwright": {
      "command": "npx",
      "description": "Playwright MCP 服务",
      "args": [
        "@playwright/mcp@latest",
        "--headless"
      ],
      "enabled": true
    },
    "fetch": {
      "command": "uvx",
      "description": "Fetch MCP 服务",
      "args": [
        "mcp-server-fetch"
      ],
      "enabled": true
    },
    "slack": {
      "command": "npx",
      "description": "Slack MCP 服务",
      "args": [
        "-y",
        "@modelcontextprotocol/server-slack"
      ],
      "env": {
        "SLACK_BOT_TOKEN": "your-bot-token",
        "SLACK_TEAM_ID": "your-team-id"
      },
      "enabled": true
    },
    "text": {
      "type": "stdio",
      "command": "npx",
      "description": "Text MCP 服务",
      "args": [
        "@playwright/mcp@latest",
        "--headless"
      ]
    }
  },
